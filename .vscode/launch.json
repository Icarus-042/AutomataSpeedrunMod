{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Automata",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${env:NIER_DIR}\\NieRAutomata.exe.unpacked.exe",
      "cwd": "${env:NIER_DIR}",
      "symbolSearchPath": "${workspaceFolder}/build/Debug",
      "preLaunchTask": "install_to_nier",
      "args": [],
      "environment": [
        {
          "name": "SteamGameId",
          "value": "524220"
        },
        {
          "name": "SteamAppId",
          "value": "524220"
        },
        {
          "name": "SteamOverlayGameId",
          "value": "524220"
        }
      ],
      "logging": {
        "moduleLoad": true,
        "trace": true
      }
    }
  ]
}
